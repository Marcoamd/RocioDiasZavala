---
import Timeline from "../components/Timeline.astro";
import Layout from "../layouts/Layout.astro";
import { AstroScript } from "astro-script";
const baseUrl = import.meta.env.BASE_URL;
title: "Experiencia";
date: "2023-11-05";
author: "Rocío Marivel Díaz Zavala";
---

<Layout title="Rocío Marivel Díaz Zavala">
  <section>
    <!-- Title -->
    <div class="flex items-center my-8">
      <h1 class="text-3xl tracking-widest font-extrabold text-dark-grey-900">
        EXPERIENCIA
      </h1>
      <div class="h-0.5 w-full ml-8 bg-violet-300"></div>
    </div>
    <!-- /Title -->

    <Timeline />
  </section>
</Layout>
<AstroScript src={baseUrl+"./../src/scripts/main_.js"} />

